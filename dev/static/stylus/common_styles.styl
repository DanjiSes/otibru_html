#langs
  /*position absolute
  top 15px
  right 15px
  z-index 99*/
  display inline-block
  
  a
    margin 0 5px
    color #fff
    text-decoration underline

.radius
  position absolute
  bottom 0
  left 0
  width 100%
  z-index 1

  img
    width 100%

#scrolltop
  position fixed
  right 15px
  bottom 15px
  z-index 999
  background #55ad67
  width 30px
  height 30px
  text-align center
  border-radius 50%
  color #fff
  line-height 27px
  font-size 25px
  transform rotate(-90deg)
  cursor pointer
  box-shadow 0px 0px 13.92px 2.08px rgba(35, 35, 35, 0.35)
  display none
  
img
  max-width 100%
  height auto

.btn
  display inline-block
  padding 20px 65px
  border-radius 45px
  color #fff
  background-color #44b353
  font-weight 500
  font-size rem(23)
  border none
  text-align center
  box-shadow 0px 0px 13.92px 2.08px rgba(35, 35, 35, 0.35)
  

.title
  padding 0 15px
  text-align center
  font-style italic
  margin 0 0 50px 0
  font-size rem(30)

section
  padding 75px 0